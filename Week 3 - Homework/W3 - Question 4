# Question 4


class MyQueue:
    def __init__(self, capacity):
        self.__capacity = capacity
        self.__queue = []
        print(f"Create a queue with capacity is {capacity}")

    def is_empty(self):
        if len(self.__queue) == 0:
            return True
        else:
            return False

    def is_full(self):
        if len(self.__queue) == self.__capacity:
            return True
        else:
            return False

    def dequeue(self):
        if self.is_empty() == True:
            return print("A queue is empty")
        else:
            return print(f"Deleted value : {self.__queue.pop(0)}")

    def enqueue(self, value):
        if self.is_full() == True:
            return print("A queue is full")
        else:
            self.__queue.append(value)
            print(f"Add value = {value} successful")

    def front(self):
        if self.is_empty():
            print("A queue is empty")
        else:
            return print(f"First value in stack :{self.__queue[0]}")


# Create a queue with capacity is 5.
queue1 = MyQueue(capacity=5)

# Checking a queue is empty.
queue1.is_empty()

# Checking a queue is full.
queue1.is_full()

# Adding value to a queue.
queue1.enqueue(1)
queue1.enqueue(2)
queue1.enqueue(3)
queue1.enqueue(4)
queue1.enqueue(5)

# Delele first value of a queue.
queue1.dequeue()

# Get first value of a queue.
queue1.front()
